@{
    ViewBag.Title = "User list";
}
<ol class="breadcrumb">
    <li><h3>@ViewBag.Title</h3></li>
</ol>

@foreach (var user in ViewBag.UsersList)
{
    <div class="panel-heading">User name: @user.UserName</div>
    <div class="panel-body">
    <i class="glyphicon glyphicon-envelope"></i> Email: <span class="label label-default">@user.Email</span>
    <br />
    </div>
    <div class="panel-footer">
        <a class="btn btn-sm btn-warning" href="/Users/Edit/@user.Id"> Edit user</a> <a class="btn btn-sm btn-success" href="/Users/Show/@user.Id"> Show user details</a>
    </div>
    <br /><br />
}